    "react": "17.0.2",
    "react-chartjs-2": "^3.0.2",
    "react-dom": "17.0.2",
    "sqlite3": "^5.0.2",
    "zod": "^3.0.0-alpha.33"
  },
  "devDependencies": {
    "@cubejs-backend/cubestore-driver": "^0.26.102",
