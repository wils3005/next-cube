    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.4.0",
    "faker": "^5.5.3",
    "npm-check": "^5.9.2",
    "postcss": "^8.2.12",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
